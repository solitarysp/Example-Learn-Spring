<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean name="studentService" scope="prototype" lazy-init="true"
          class="com.higgsup.example_config.xml.service.StudentService" >
        <property name="addressService" ref="addressStudent"/>
    </bean>

    <bean name="addressStudent" class="com.higgsup.example_config.xml.service.AddressService"/>

</beans>